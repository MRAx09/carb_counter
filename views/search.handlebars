    <div id="leftWindow">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <button class="nav-link link-success" id="search-tab" data-bs-toggle="tab" data-bs-target="#search"
                        type="button" role="tab" aria-controls="search" aria-selected="true">Search Results</button>
                </li>
        
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="search" role="tabpanel" aria-labelledby="search-tab">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th scope="col">Add to <br>Current Meal</th>
                                <th scope="col">Food Name</th>
                                <th scope="col">Serving</th>
                                <th scope="col">Carbs (g)</th>
                            </tr>
                        </thead>
                        <tbody>
                           <tr>
                                <td class = "addToCur"><button type="button" class="btn btn-sm btn-outline search-to-current" search-to-current-id={{this.food_name}} data-id="{{this.food_name}}">+</button>
                                <td>{{ this.food_name }}</td>
                                <td>{{ this.serving_qty }} {{ this.serving_unit }} <br> ({{ this.serving_weight_grams }}g)</td>
                                <td>{{ this.nf_total_carbohydrates }}g     <br>   (includes {{ this.nf_dietary_fiber }}g fiber)</td>
                                <br>
                                    <div class="input-group mb-3">
                                        <a href='/'><button class="btn btn-outline-success" type="button">Go Back</button></a>
                                    </div>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

 
<script src="/js/currentmeal.js"></script>

{{#if logged_in}}
<script src="/js/logout.js"></script>
{{/if}}