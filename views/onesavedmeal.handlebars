    <div id="leftWindow">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <button class="nav-link link-success" id="search-tab" data-bs-toggle="tab" data-bs-target="#search"
                        type="button" role="tab" aria-controls="search" aria-selected="true">Search Results</button>
                </li>
        
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="onemeal" role="tabpanel" aria-labelledby="onemeal-tab">
                    <table class="table table-striped table-hover">
                        <thead>
                          <p>{{meal.meal_name}}</p>
                            <tr>
                                <th scope="col">Add to <br>Current <br>Meal</th>
                                <th scope="col">Food Name</th>
                                <th scope="col">Serving</th>
                                <th scope="col">Carbs (g)</th>
                            </tr>
                        </thead>
                        <tbody>
                          {{#each meal.food as |food|}}
                           <tr>
                             
                                <td class = "addToCur"><button type="button" class="btn btn-sm btn-outline addBtn" addToCurBtn-id={{this.id}} data-id="{{this.id}}">+</button>
                                <td>{{ food.food_name }}</td>
                                <td>{{ food.serving_qty }} {{ food.serving_unit }} <br> ({{ food.serving_weight_grams }}g)</td>
                                <td>{{ food.nf_total_carbohydrates }}g     <br>   (includes {{ food.nf_dietary_fiber }}g fiber)</td>
                           {{/each}}     
                                <br>
                                    <div class="input-group mb-3">
                                        <a href='/'><button class="btn btn-outline-success" type="button">Go Back</button></a>
                                    </div>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

<script src="/js/currentmeal.js"></script>




